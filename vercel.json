{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/vue-component-collection/(.*)",
      "destination": "/vue-component-collection/index.html"
    }
  ],
  "headers": [
    {
      "source": "/vue-component-collection/(css|js|images)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

