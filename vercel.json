{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "index.html"
    },
    {
      "source": "/(.*)",
      "destination": "index.html"
    }
  ],
  "headers": [
    {
      "source": "(css|js|images)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

